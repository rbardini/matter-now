{
  "name": "matter-now",
  "version": "0.0.0-development",
  "description": "Append the current date to the YAML front matter of Markdown files",
  "keywords": [
    "front-matter",
    "yaml",
    "markdown",
    "date",
    "time",
    "datetime",
    "timestamp",
    "now"
  ],
  "homepage": "https://github.com/rbardini/matter-now",
  "bugs": {
    "url": "https://github.com/rbardini/matter-now/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rbardini/matter-now.git"
  },
  "license": "MIT",
  "author": "Rafael Bardini <rafael@rbardini.com> (https://rbardini.com)",
  "type": "module",
  "main": "index.js",
  "bin": "./bin/matter-now.js",
  "files": [
    "bin/*",
    "*.js"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore . bin/*",
    "prepare": "husky install"
  },
  "dependencies": {
    "commander": "7.2.0",
    "gray-matter": "4.0.3",
    "moment": "2.29.1"
  },
  "devDependencies": {
    "eslint": "7.26.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.23.2",
    "husky": "6.0.0",
    "lint-staged": "11.0.0"
  }
}
